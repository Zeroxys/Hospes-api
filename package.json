{
  "name": "hospes-api",
  "version": "1.0.0",
  "description": "Hospes REST API",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && ava test/**test.js --tap | tap-nyan"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "modules": true
  },
  "NODE_ENV":"test",
  "babel": {
    "presets": [
      "@ava/stage-4"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "author": "Miguel Zavala <miguelzavalac@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-register": "^6.23.0",
    "standard": "^9.0.0",
    "supports-color": "^3.2.3",
    "tap-nyan": "^1.1.0",
    "test-listen": "^1.0.1"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "http-hash": "^2.0.0",
    "micro": "^7.0.6",
    "request": "^2.80.0",
    "request-promise": "^4.1.1",
    "uuid-base62": "^0.1.0"
  }
}
